{
  "name": "desyjs",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
"dev:back": "ts-node-dev --respawn --transpile-only public/index.ts",
    "dev:front": "cd frontend && npm run dev",
    "dev": "concurrently --kill-others \"npm run dev:back\" \"npm run dev:front\"",
    "build:front": "cd frontend && npm run build",
    "console": "npx ts-node bin/console.ts",
    "make:controller": "npm run console -- make:controller",
    "make:entity": "npm run console -- make:entity",
    "make:crud": "npm run console -- make:crud",
    "make:migration": "npm run console -- make:migration",
    "make:auth": "npm run console -- make:auth",
    "migrate": "npm run console -- doctrine:migrations:migrate",
    "debug:router": "npm run console -- debug:router"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "chalk": "^4.1.2",
    "commander": "^14.0.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "inquirer": "^13.1.0",
    "jsonwebtoken": "^9.0.3",
    "ora": "^9.0.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
